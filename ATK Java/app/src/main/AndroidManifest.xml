<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.atkmobile">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".DailyIncome_2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".oneYearSale"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DetailStorage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Storage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Mostwanted"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Warehouse"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NAB">
        </activity>
        <activity android:name=".splashScreen"
            android:theme="@style/AppTheme.NAB">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>